datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Siswa {
  id     Int    @id @default(autoincrement())
  nama   String
  kelas  String
  umur   Int?
}

model User {
  id        Int      @id @default(autoincrement())
  username  String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Product {
  id        Int      @id @default(autoincrement())
  name      String
  price     Int
  stock     Int
  createdAt DateTime @default(now())
}